{
  "manifest_version": 2,

  "name": "Tom LinkLive Details",
  "description": "This extension shows a LinkLive details of the current page",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "mainPage.html",
    "default_title": "Tom is here!"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
